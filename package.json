{
  "name": "omi-snippets",
  "displayName": "Omi Snippets",
  "description": "A beautify Visual Studio Code extension for .omi .eno .vue or scss file",
  "version": "1.9.0",
  "author": "Eno Yao",
  "publisher": "Wscats",
  "icon": "logos/omi-logo.png",
  "keywords": [
    "omi",
    "omijs",
    "omi.js",
    "eno",
    "eno yao",
    "yao",
    "wscats",
    "yjl",
    "css",
    "scss",
    "sass",
    "lemon",
    "laoyao",
    "one",
    "jing",
    "jialong",
    "yaojialong",
    "xie",
    "DK",
    "lan",
    "wang",
    "sha",
    "react",
    "jsx",
    "sfc",
    "component",
    "vue",
    "single file component",
    "老姚",
    "一一",
    "htm"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "scripts": {
    "build": "vsce package"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wscats/omi-snippets"
  },
  "bugs": {
    "url": "https://github.com/Wscats/omi-snippets/issues",
    "email": "kalone.cool@gmail.com"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "contributes": {
    "languages": [
      {
        "id": "omi",
        "aliases": [
          "Omi",
          "eno",
          "html",
          "Eno",
          "omi",
          "Eno.js",
          "eno",
          "html"
        ],
        "extensions": [
          ".omi"
        ],
        "configuration": "./omi.configuration.json"
      },
      {
        "id": "eno",
        "aliases": [
          "Omi",
          "Omi.js",
          "omi",
          "Eno.js",
          "eno",
          "html"
        ],
        "extensions": [
          ".eno"
        ],
        "configuration": "./omi.configuration.json"
      },
      {
        "id": "vue",
        "aliases": [
          "vue"
        ],
        "extensions": [
          ".vue"
        ]
      },
      {
        "id": "html",
        "extensions": [
          ".omi",
          ".eno",
          ".html",
          ".htm",
          ".shtml",
          ".xhtml",
          ".mdoc",
          ".jsp",
          ".asp",
          ".aspx",
          ".jshtm",
          ".volt",
          ".ejs",
          ".rhtml"
        ],
        "aliases": [
          "HTML",
          "htm",
          "html",
          "xhtml"
        ]
      }
    ],
    "grammars": [
      {
        "language": "omi",
        "scopeName": "text.html.omi",
        "path": "./syntaxes/omi.tmLanguage.json"
      },
      {
        "language": "eno",
        "scopeName": "text.html.omi",
        "path": "./syntaxes/omi.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "omi",
        "path": "./snippets/omi.json"
      },
      {
        "language": "omi",
        "path": "./snippets/js.json"
      },
      {
        "language": "omi",
        "path": "./snippets/html.json"
      },
      {
        "language": "omi",
        "path": "./snippets/css.json"
      },
      {
        "language": "eno",
        "path": "./snippets/omi.json"
      },
      {
        "language": "eno",
        "path": "./snippets/js.json"
      },
      {
        "language": "eno",
        "path": "./snippets/html.json"
      },
      {
        "language": "eno",
        "path": "./snippets/css.json"
      }
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "omil": "^1.8.9",
    "prettier": "1.17.1"
  }
}
