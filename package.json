{
	"name": "omi-snippets",
	"description": "A beautify Visual Studio Code extension for .omi or .eno file",
	"version": "0.2.5",
	"author": "Eno Yao",
	"publisher": "Wscats",
	"icon": "images/omi-logo.png",
	"keywords": [
		"omi",
		"omijs",
		"omi.js",
		"eno",
		"eno yao",
		"yao",
		"wscats",
		"yjl"
	],
	"scripts": {
		"build": "vsce package"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Wscats/omi-snippets"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"iconThemes": [
			{
				"id": "vscode-icons",
				"label": "VSCode Icons",
				"path": "./icons/icons.json"
			}
		],
		"languages": [
			{
				"id": "omi",
				"aliases": [
					"Omi.js",
					"omi",
					"Eno.js",
					"eno"
				],
				"extensions": [
					".omi"
				],
				"configuration": "./omi.configuration.json"
			},
			{
				"id": "eno",
				"aliases": [
					"Omi.js",
					"omi",
					"Eno.js",
					"eno",
					"yao"
				],
				"extensions": [
					".eno"
				],
				"configuration": "./omi.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "omi",
				"scopeName": "text.html.omi",
				"path": "./syntaxes/omi.tmLanguage.json"
			},
			{
				"language": "eno",
				"scopeName": "text.html.omi",
				"path": "./syntaxes/omi.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "omi",
				"path": "./snippets/snippets.json"
			},
			{
				"language": "eno",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"license": "MIT"
}